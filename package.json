{
  "name": "sqld",
  "version": "0.0.0-pre.0",
  "description": "A wrapper for libsql-server (sqld) with platform-specific binaries",
  "scripts": {
    "build": "node scripts/build.js",
    "check-version": "node scripts/check-version.js",
    "extract-binaries": "node scripts/extract-binaries.js",
    "publish-all": "node scripts/publish-all.js"
  },
  "optionalDependencies": {
    "@sqld/darwin-arm64": "workspace:*",
    "@sqld/darwin-x64": "workspace:*",
    "@sqld/linux-arm64": "workspace:*",
    "@sqld/linux-x64": "workspace:*"
  },
  "devDependencies": {
    "tar": "^7.4.3"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vltpkg/sqld.git"
  },
  "keywords": [
    "libsql",
    "sqlite",
    "database",
    "sqld",
    "turso"
  ],
  "author": "darcyclarke <darcy@darcyclarke.me>",
  "license": "MIT"
}
